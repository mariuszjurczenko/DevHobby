@model IEnumerable<Category>

@foreach (var category in Model)
{
    <li class="nav-item">
        <a asp-controller="Course"
           asp-action="List"
           asp-route-category="@category.Name"
           class="nav-link">
            @category.Name
        </a>
    </li>
}
<li class="nav-item">
    <a asp-controller="Course"
       asp-action="List"
       asp-route-Category=""
       class="nav-link">
        Wszystkie kursy
    </a>
</li>
